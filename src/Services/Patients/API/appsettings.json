{
  "app": {
    "name": "Scheduler API",
    "version": "v1",
    "project": "CompanyName.MyProjectName.Scheduler",
    "modulepart": "CompanyName.MyProjectName.Modules."
  },
  "asyncAPI": {
    "enabled": true,
    "title": "Scheduler API",
    "version": "1.0",
    "description": "Schedulers Async API messages",
    "servers": {
      "RabbitMQ local": {
        "url": "localhost:5672",
        "protocol": "AMQP"
      }
    },
    "bindings": {
      "customers": {
        "amqp": {
          "type": "routingKey",
          "exchange": {
            "name": "scheduler",
            "virtualHost": "/"
          }
        }
      }
    }
  },
  //"auth": {
  //  "certificate": {
  //    "location": "certs/localhost.pfx",
  //    "password": "test",
  //    "rawData": ""
  //  },
  //  "jwt": {
  //    "issuer": "micro",
  //    "audience": "micro",
  //    "validIssuer": "micro",
  //    "validAudience": "micro",
