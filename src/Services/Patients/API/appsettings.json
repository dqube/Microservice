  //    "validateIssuer": true,
  //    "validateLifetime": true,
  //    "expiry": "07.00:00:00"
  //  }
  //},
  "consul": {
    "enabled": false,
    "url": "http://localhost:8500",
    "service": {
      "name": "customers",
      "url": "http://host.docker.internal:5010"
    },
    "healthCheck": {
      "endpoint": "ping",
      "interval": "00.00:00:05",
      "deregisterInterval": "00.00:00:05"
    }
  },
  "cors": {
    "enabled": true,
    "allowCredentials": true,
    "allowedOrigins": [],
    "allowedMethods": [
      "POST",
      "PUT",
      "DELETE"
    ],
    "allowedHeaders": [
      "Content-Type",
      "Authorization"
    ],
    "exposedHeaders": [
      "Resource-ID"
    ]
  },
  "fabio": {
    "enabled": true,
    "url": "http://localhost:9999"
  },
  "httpClient": {
    "name": "",
    "type": "",
    "certificate": {
      "location": "certs/localhost.pfx",
      "password": "test"
    },
    "resiliency": {
      "retries": 3,
      "retryInterval": "00.00:00:03",
      "exponential": false
    }
  },
  "inbox": {
    "enabled": true,
    "cleanupInterval": "00.01:00:00"
  },
  "metrics": {
    "enabled": true,
    "endpoint": "/metrics",
    "exporter": "oltp",
    "url": "http://localhost:4317"
  },
  "messaging": {
    "resiliency": {
      "retries": 3,
      "retryInterval": "00.00:00:03",
      "exponential": false
    }
  },
  "outbox": {
    "enabled": true,
    "senderInterval": "00.00:00:10",
    "cleanupInterval": "00.01:00:00"
  },
  "sqlserver": {
    "connectionString": "Data Source=(localdb)\\MSSQLlocalDB;Initial Catalog=Gatherly;Integrated Security=True;Trust Server Certificate=True;Command Timeout=300",
    "connectionStrings": {
      "patient": "Data Source=(localdb)\\MSSQLlocalDB;Initial Catalog=Gatherly;Integrated Security=True;Trust Server Certificate=True;Command Timeout=300"
    }
  },
  "rabbitMQ": {
    "enabled": true,
    "connectionString": "host=localhost;port=5672;virtualHost=/;username=guest;password=guest"
  },
  "azureServiceBus": {
    "enabled": false,
    "connectionString": "Endpoint=sb://subscrify.servicebus.windows.net/;SharedAccessKeyName=secret;SharedAccessKey=secret"
  },
  "security": {
    "encryption": {
      "key": "SsAGentgn5E9vQRt3XcrcDXY9ZYqFVLw"
    }
  },
  "swagger": {
    "enabled": true,
    "title": "Scheduler API",
    "version": "v1",
    "route": "swagger"
  },
  "quartz": {
    "jobs": {
      "scheduler": {
        "interval": 10,
        "name": "scheduler"
      },
      "appointment": {
        "interval": 30,
        "name": "scheduler"
      }
    }
  },
  "tracing": {
    "enabled": true,
    "exporter": "oltp",
    "url": "http://localhost:4317",
    "jaeger": {
      "agentHost": "localhost",
      "agentPort": 6831,
      "maxPayloadSizeInBytes": null,
      "exportProcessorType": "batch"
    }
  },
  "vault": {
    "enabled": true,
    "url": "http://localhost:8200",
    "authentication": {
      "type": "token",
      "token": {
        "token": "secret"
      }
    },
    "KV": {
      "enabled": true,
      "path": "customers"
    },
    "lease": {
      "postgres": {
        "enabled": true,
        "type": "database",
        "roleName": "micro-customers",
        "templates": {
          "connectionString": "Host=localhost;Database=micro-customers;Username={{username}};Password={{password}}"
        }
      }
    },
    "PKI": {
      "enabled": true,
      "roleName": "micro-customers",
      "commonName": "customers.micro.io",
      "importPrivateKey": true,
      "httpHandler": {
        "enabled": true,
        "certificate": "micro-customers"
      }
    }
  },
  "serilog": {
    "console": {
      "enabled": true
    },
    "excludePaths": [
      "/",
      "/metrics",
      "/ping"
    ],
    "excludeProperties": [
      "api_key",
      "access_key",
      "password",
      "email"
    ],
    "file": {
      "enabled": false,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "seq": {
      "enabled": true,
      "url": "http://localhost:4317",
      "apiKey": "secret"
    },
    "tags": {}
  },
